URxvt*termName: rxvt-unicode-256color
URxvt*saveLines: 10000
URxvt*background:  #1b1d1e
URxvt*foreground:  #a8a8a8
URxvt*scrollBar_right:	true
URxvt*scrollBar:	false
URxvt*inheritPixmap:	false
URxvt*geometry:		84x24
URxvt*tint:		white
URxvt*font: xft:gohufont:style=Regular:pixelsize=13
URxvt*boldFont: xft:gohufont:style=Bold:pixelsize=13
URxvt*color0:     #1b1d1e
URxvt*color1:     #cc5555
URxvt*color2:     #739f12
URxvt*color3:     #ccc355
URxvt*color4:     #4072bc
URxvt*color5:     #6f38df
URxvt*color6:     #42aec2
URxvt*color7:     #b9b9b9
URxvt*color8:     #505354
URxvt*color9:     #ff8888
URxvt*color10:    #c2e86f
URxvt*color11:    #fffcbb
URxvt*color12:    #99bbee
URxvt*color13:    #bd9cff
URxvt*color14:    #9dedfb
URxvt*color15:    #ffffff
URxvt*urgentOnBell:  true
URxvt*visualBell:    true
URxvt*fading:           20
URxvt*fadeColor:        black
URxvt*blurRadius:       5
URxvt.matcher.rend.0: -Uline
URxvt*perl-ext: default,matcher
URxvt.perl-ext-common: ...,keyboard-select
URxvt.keysym.M-Escape: perl:keyboard-select:activate
URxvt*perl-ext-common: default,matcher,digital-clock,monit
URxvt*url-Launcher:firefox
URxvt*jumpScroll: true
URxvt*scrollWithBuffer: true
URxvt*scrollTtyOutput: false
URxvt*scrollTtyKeypress: true
URxvt*cursorBlink: true
 
URxvt.matcher.button: 1

## xterm config
xterm*termName: xterm
xterm*background: #1b1d1e
xterm*foreground: #A8A8A8
